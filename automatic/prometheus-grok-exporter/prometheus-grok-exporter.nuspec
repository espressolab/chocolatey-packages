  <metadata>
    <id>prometheus-grok-exporter</id>
    <version>0.2.8</version>
    <packageSourceUrl>https://github.com/espressolab/chocolatey-packages/tree/master/automatic/prometheus-grok-exporter</packageSourceUrl>
    <owners>Espresso Lab</owners>
    <title>Prometheus Grok exporter</title>
    <authors>Fabian Stäber</authors>
    <projectUrl>https://github.com/fstab/grok_exporter</projectUrl>
    <iconUrl>https://chocolatey.org/content/packageimages/prometheus.2.0.0.png</iconUrl>
